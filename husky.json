{
    "husky": {
        "hooks": {
          "pre-commit": "lint-staged"
        }
      },
      "lint-staged": {
        "linters": {
          "src/**/*.{js,jsx}": [
            "eslint --fix",
            "git add"
          ]
        },
        "ignore": [
          "src/assets/**/*"
        ]
      },
      "config": {
        "commitizen": {
          "path": "./node_modules/cz-conventional-changelog"
        }
      }
}
